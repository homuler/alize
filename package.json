{
  "name": "alizejs",
  "version": "0.0.0",
  "description": "REST Client in JavaScript",
  "config": {
    "node_path": "./src/lib:./test/lib:./test:$NODE_PATH"
  },
  "scripts": {
    "dev": "node scripts/devserver.js",
    "build": "$(npm bin)/webpack --config config/webpack/lib.config.js",
    "lint": "$(npm bin)/eslint --config config/eslint/index.js src/ examples/",
    "test": "NODE_PATH=$npm_package_config_node_path $(npm bin)/babel-node ./node_modules/.bin/mocha --opts config/mocha.opts",
    "test:cover": "NODE_PATH=$npm_package_config_node_path $(npm bin)/babel-node $(npm bin)/isparta cover --config config/istanbul.yml ./node_modules/.bin/mocha -- --opts config/mocha.opts",
    "test:server": "node scripts/testserver.js"
  },
  "keywords": [
    "rest-client"
  ],
  "author": "homuler",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.1.0",
    "estraverse-fb": "^1.3.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.0",
    "global": "^4.3.0",
    "imports-loader": "^0.6.5",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "lodash": "^4.5.0",
    "query-string": "^3.0.0",
    "whatwg-fetch": "^0.11.0"
  }
}
